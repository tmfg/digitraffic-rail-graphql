type Query {
    animals(id: ID): [Animal]
    train(trainNumber:Int, departureDate:Date): Train
}

type Train {
    trainNumber:Int
    departureDate:Date
    version:String
}

scalar Date

type Animal {
    id: ID
    name: String
    color: String
    countryIds: [ID]
    countries: [Country]
}

type Country {
    id: ID
    name: String
}
